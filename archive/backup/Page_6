  <script>
    function togglePrivacy() {
      const content = document.getElementById('privacy-content');
      const arrow = document.getElementById('privacy-arrow');
      content.classList.toggle('active');
      arrow.style.transform = content.classList.contains('active') ? 'rotate(180deg)' : 'rotate(0deg)';
    }

    const defaultConfig = {
      background_color: '#F8F9FC',
      surface_color: '#FFFFFF',
      text_color: '#0A2540',
      primary_action_color: '#0A2540',
      secondary_action_color: '#5CE1E6',
      font_family: 'Inter',
      font_size: 16,
      page_title: 'Your Profile',
      page_subtitle: 'Manage your personal information, account settings, and AI preferences.',
      user_name: 'Qurbanali Feyzullayev',
      user_level: 'BSc Biology',
      user_email: 'qurbanali.f@example.com',
      user_phone: '+1 (555) 123-4567',
      account_settings_title: 'Account Settings',
      documents_title: 'Your Uploaded Documents',
      ai_preferences_title: 'AI & Recommendations Settings',
      privacy_title: 'Privacy & Data Management',
      footer_text: 'NoApplAI — Smarter academic application journeys powered by AI.'
    };

    let config = { ...defaultConfig };

    async function onConfigChange(newConfig) {
      config = { ...config, ...newConfig };
      
      const baseSize = config.font_size || defaultConfig.font_size;
      const customFont = config.font_family || defaultConfig.font_family;
      const baseFontStack = 'Inter, sans-serif';
      const headingFontStack = 'Montserrat, sans-serif';

      document.body.style.backgroundColor = config.background_color || defaultConfig.background_color;
      document.body.style.fontFamily = `${customFont}, ${baseFontStack}`;
      document.body.style.fontSize = `${baseSize}px`;

      const pageTitle = document.getElementById('page-title');
      if (pageTitle) {
        pageTitle.textContent = config.page_title || defaultConfig.page_title;
        pageTitle.style.color = config.text_color || defaultConfig.text_color;
        pageTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        pageTitle.style.fontSize = `${baseSize * 2.5}px`;
      }

      const pageSubtitle = document.getElementById('page-subtitle');
      if (pageSubtitle) {
        pageSubtitle.textContent = config.page_subtitle || defaultConfig.page_subtitle;
        pageSubtitle.style.fontFamily = `${customFont}, ${baseFontStack}`;
        pageSubtitle.style.fontSize = `${baseSize * 1.125}px`;
      }

      const userName = document.getElementById('user-name');
      if (userName) {
        userName.textContent = config.user_name || defaultConfig.user_name;
        userName.style.color = config.text_color || defaultConfig.text_color;
        userName.style.fontFamily = `${customFont}, ${headingFontStack}`;
        userName.style.fontSize = `${baseSize * 1.5}px`;
      }

      const userLevel = document.getElementById('user-level');
      if (userLevel) {
        userLevel.textContent = config.user_level || defaultConfig.user_level;
        userLevel.style.fontFamily = `${customFont}, ${baseFontStack}`;
        userLevel.style.fontSize = `${baseSize * 1.125}px`;
      }

      const userEmail = document.getElementById('user-email');
      if (userEmail) {
        userEmail.textContent = config.user_email || defaultConfig.user_email;
        userEmail.style.fontFamily = `${customFont}, ${baseFontStack}`;
      }

      const userPhone = document.getElementById('user-phone');
      if (userPhone) {
        userPhone.textContent = config.user_phone || defaultConfig.user_phone;
        userPhone.style.fontFamily = `${customFont}, ${baseFontStack}`;
      }

      const accountSettingsTitle = document.getElementById('account-settings-title');
      if (accountSettingsTitle) {
        accountSettingsTitle.textContent = config.account_settings_title || defaultConfig.account_settings_title;
        accountSettingsTitle.style.color = config.text_color || defaultConfig.text_color;
        accountSettingsTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        accountSettingsTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const documentsTitle = document.getElementById('documents-title');
      if (documentsTitle) {
        documentsTitle.textContent = config.documents_title || defaultConfig.documents_title;
        documentsTitle.style.color = config.text_color || defaultConfig.text_color;
        documentsTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        documentsTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const aiPreferencesTitle = document.getElementById('ai-preferences-title');
      if (aiPreferencesTitle) {
        aiPreferencesTitle.textContent = config.ai_preferences_title || defaultConfig.ai_preferences_title;
        aiPreferencesTitle.style.color = config.text_color || defaultConfig.text_color;
        aiPreferencesTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        aiPreferencesTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const privacyTitle = document.getElementById('privacy-title');
      if (privacyTitle) {
        privacyTitle.textContent = config.privacy_title || defaultConfig.privacy_title;
        privacyTitle.style.color = config.text_color || defaultConfig.text_color;
        privacyTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        privacyTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const footerText = document.getElementById('footer-text');
      if (footerText) {
        footerText.textContent = config.footer_text || defaultConfig.footer_text;
        footerText.style.fontFamily = `${customFont}, ${baseFontStack}`;
        footerText.style.fontSize = `${baseSize * 0.875}px`;
      }

      const allHeadings = document.querySelectorAll('h1, h2, h3, h4');
      allHeadings.forEach(heading => {
        if (!heading.id) {
          heading.style.color = config.text_color || defaultConfig.text_color;
          heading.style.fontFamily = `${customFont}, ${headingFontStack}`;
        }
      });

      const allSurfaces = document.querySelectorAll('.bg-white');
      allSurfaces.forEach(surface => {
        surface.style.backgroundColor = config.surface_color || defaultConfig.surface_color;
      });
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              config.background_color = value;
              window.elementSdk.setConfig({ background_color: value });
            }
          },
          {
            get: () => config.surface_color || defaultConfig.surface_color,
            set: (value) => {
              config.surface_color = value;
              window.elementSdk.setConfig({ surface_color: value });
            }
          },
          {
            get: () => config.text_color || defaultConfig.text_color,
            set: (value) => {
              config.text_color = value;
              window.elementSdk.setConfig({ text_color: value });
            }
          },
          {
            get: () => config.primary_action_color || defaultConfig.primary_action_color,
            set: (value) => {
              config.primary_action_color = value;
              window.elementSdk.setConfig({ primary_action_color: value });
            }
          },
          {
            get: () => config.secondary_action_color || defaultConfig.secondary_action_color,
            set: (value) => {
              config.secondary_action_color = value;
              window.elementSdk.setConfig({ secondary_action_color: value });
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            config.font_family = value;
            window.elementSdk.setConfig({ font_family: value });
          }
        },
        fontSizeable: {
          get: () => config.font_size || defaultConfig.font_size,
          set: (value) => {
            config.font_size = value;
            window.elementSdk.setConfig({ font_size: value });
          }
        }
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ['page_title', config.page_title || defaultConfig.page_title],
        ['page_subtitle', config.page_subtitle || defaultConfig.page_subtitle],
        ['user_name', config.user_name || defaultConfig.user_name],
        ['user_level', config.user_level || defaultConfig.user_level],
        ['user_email', config.user_email || defaultConfig.user_email],
        ['user_phone', config.user_phone || defaultConfig.user_phone],
        ['account_settings_title', config.account_settings_title || defaultConfig.account_settings_title],
        ['documents_title', config.documents_title || defaultConfig.documents_title],
        ['ai_preferences_title', config.ai_preferences_title || defaultConfig.ai_preferences_title],
        ['privacy_title', config.privacy_title || defaultConfig.privacy_title],
        ['footer_text', config.footer_text || defaultConfig.footer_text]
      ]);
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
