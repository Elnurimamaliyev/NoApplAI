  <script>
    const defaultConfig = {
      background_color: '#F8F9FC',
      surface_color: '#FFFFFF',
      text_color: '#0A2540',
      primary_action_color: '#0A2540',
      secondary_action_color: '#5CE1E6',
      font_family: 'Inter',
      font_size: 16,
      user_first_name: 'Student',
      welcome_subtext: 'Continue managing your university applications and recommendations.',
      progress_title: 'Application Progress',
      documents_title: 'Documents & Requirements',
      recommendations_title: 'Recommended Programs',
      deadlines_title: 'Upcoming Deadlines',
      activity_title: 'Recent Activity',
      footer_text: 'Powered by NoApplAI ‚Äî Personalized AI-assisted academic opportunities.'
    };

    let config = { ...defaultConfig };

    async function onConfigChange(newConfig) {
      config = { ...config, ...newConfig };
      
      const baseSize = config.font_size || defaultConfig.font_size;
      const customFont = config.font_family || defaultConfig.font_family;
      const baseFontStack = 'Inter, sans-serif';
      const headingFontStack = 'Montserrat, sans-serif';

      document.body.style.backgroundColor = config.background_color || defaultConfig.background_color;
      document.body.style.fontFamily = `${customFont}, ${baseFontStack}`;
      document.body.style.fontSize = `${baseSize}px`;

      const welcomeHeading = document.getElementById('welcome-heading');
      if (welcomeHeading) {
        const firstName = config.user_first_name || defaultConfig.user_first_name;
        welcomeHeading.textContent = `Welcome back, ${firstName} üëã`;
        welcomeHeading.style.color = config.text_color || defaultConfig.text_color;
        welcomeHeading.style.fontFamily = `${customFont}, ${headingFontStack}`;
        welcomeHeading.style.fontSize = `${baseSize * 2.5}px`;
      }

      const welcomeSubtext = document.getElementById('welcome-subtext');
      if (welcomeSubtext) {
        welcomeSubtext.textContent = config.welcome_subtext || defaultConfig.welcome_subtext;
        welcomeSubtext.style.fontFamily = `${customFont}, ${baseFontStack}`;
        welcomeSubtext.style.fontSize = `${baseSize * 1.125}px`;
      }

      const avatarInitial = document.getElementById('avatar-initial');
      if (avatarInitial) {
        const firstName = config.user_first_name || defaultConfig.user_first_name;
        avatarInitial.textContent = firstName.charAt(0).toUpperCase();
      }

      const progressTitle = document.getElementById('progress-title');
      if (progressTitle) {
        progressTitle.textContent = config.progress_title || defaultConfig.progress_title;
        progressTitle.style.color = config.text_color || defaultConfig.text_color;
        progressTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        progressTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const documentsTitle = document.getElementById('documents-title');
      if (documentsTitle) {
        documentsTitle.textContent = config.documents_title || defaultConfig.documents_title;
        documentsTitle.style.color = config.text_color || defaultConfig.text_color;
        documentsTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        documentsTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const recommendationsTitle = document.getElementById('recommendations-title');
      if (recommendationsTitle) {
        recommendationsTitle.textContent = config.recommendations_title || defaultConfig.recommendations_title;
        recommendationsTitle.style.color = config.text_color || defaultConfig.text_color;
        recommendationsTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        recommendationsTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const deadlinesTitle = document.getElementById('deadlines-title');
      if (deadlinesTitle) {
        deadlinesTitle.textContent = config.deadlines_title || defaultConfig.deadlines_title;
        deadlinesTitle.style.color = config.text_color || defaultConfig.text_color;
        deadlinesTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        deadlinesTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const activityTitle = document.getElementById('activity-title');
      if (activityTitle) {
        activityTitle.textContent = config.activity_title || defaultConfig.activity_title;
        activityTitle.style.color = config.text_color || defaultConfig.text_color;
        activityTitle.style.fontFamily = `${customFont}, ${headingFontStack}`;
        activityTitle.style.fontSize = `${baseSize * 1.5}px`;
      }

      const footerText = document.getElementById('footer-text');
      if (footerText) {
        footerText.textContent = config.footer_text || defaultConfig.footer_text;
        footerText.style.fontFamily = `${customFont}, ${baseFontStack}`;
        footerText.style.fontSize = `${baseSize * 0.875}px`;
      }

      const allHeadings = document.querySelectorAll('h1, h2, h3, h4');
      allHeadings.forEach(heading => {
        if (heading.id !== 'welcome-heading' && heading.id !== 'progress-title' && 
            heading.id !== 'documents-title' && heading.id !== 'recommendations-title' && 
            heading.id !== 'deadlines-title' && heading.id !== 'activity-title') {
          heading.style.color = config.text_color || defaultConfig.text_color;
          heading.style.fontFamily = `${customFont}, ${headingFontStack}`;
          
          if (heading.tagName === 'H2') {
            heading.style.fontSize = `${baseSize * 1.5}px`;
          } else if (heading.tagName === 'H3') {
            heading.style.fontSize = `${baseSize * 1.125}px`;
          }
        }
      });

      const allSurfaces = document.querySelectorAll('.bg-white');
      allSurfaces.forEach(surface => {
        surface.style.backgroundColor = config.surface_color || defaultConfig.surface_color;
      });

      const allSecondaryButtons = document.querySelectorAll('.bg-\\[\\#5CE1E6\\]');
      allSecondaryButtons.forEach(button => {
        button.style.backgroundColor = config.secondary_action_color || defaultConfig.secondary_action_color;
      });

      const allPrimaryButtons = document.querySelectorAll('.bg-\\[\\#0A2540\\]');
      allPrimaryButtons.forEach(button => {
        button.style.backgroundColor = config.primary_action_color || defaultConfig.primary_action_color;
      });
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              config.background_color = value;
              window.elementSdk.setConfig({ background_color: value });
            }
          },
          {
            get: () => config.surface_color || defaultConfig.surface_color,
            set: (value) => {
              config.surface_color = value;
              window.elementSdk.setConfig({ surface_color: value });
            }
          },
          {
            get: () => config.text_color || defaultConfig.text_color,
            set: (value) => {
              config.text_color = value;
              window.elementSdk.setConfig({ text_color: value });
            }
          },
          {
            get: () => config.primary_action_color || defaultConfig.primary_action_color,
            set: (value) => {
              config.primary_action_color = value;
              window.elementSdk.setConfig({ primary_action_color: value });
            }
          },
          {
            get: () => config.secondary_action_color || defaultConfig.secondary_action_color,
            set: (value) => {
              config.secondary_action_color = value;
              window.elementSdk.setConfig({ secondary_action_color: value });
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            config.font_family = value;
            window.elementSdk.setConfig({ font_family: value });
          }
        },
        fontSizeable: {
          get: () => config.font_size || defaultConfig.font_size,
          set: (value) => {
            config.font_size = value;
            window.elementSdk.setConfig({ font_size: value });
          }
        }
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ['user_first_name', config.user_first_name || defaultConfig.user_first_name],
        ['welcome_subtext', config.welcome_subtext || defaultConfig.welcome_subtext],
        ['progress_title', config.progress_title || defaultConfig.progress_title],
        ['documents_title', config.documents_title || defaultConfig.documents_title],
        ['recommendations_title', config.recommendations_title || defaultConfig.recommendations_title],
        ['deadlines_title', config.deadlines_title || defaultConfig.deadlines_title],
        ['activity_title', config.activity_title || defaultConfig.activity_title],
        ['footer_text', config.footer_text || defaultConfig.footer_text]
      ]);
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
