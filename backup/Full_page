  <script>
    // State management
    let currentPage = 'landing';
    
    // Navigation system
    function navigateToPage(pageName) {
      // Hide all pages
      document.querySelectorAll('.page').forEach(page => {
        page.classList.remove('active');
      });
      
      // Show selected page
      const targetPage = document.getElementById('page-' + pageName);
      if (targetPage) {
        targetPage.classList.add('active');
        currentPage = pageName;
        
        // Scroll to top
        window.scrollTo(0, 0);
        
        // Update nav links
        updateNavLinks(pageName);
        
        // Close notifications dropdown
        const notificationsDropdown = document.getElementById('notifications-dropdown');
        if (notificationsDropdown) {
          notificationsDropdown.classList.remove('show');
        }
      }
    }
    
    // Update navigation active state
    function updateNavLinks(activePage) {
      document.querySelectorAll('.nav-link').forEach(link => {
        link.classList.remove('active');
        if (link.getAttribute('data-page') === activePage) {
          link.classList.add('active');
        }
      });
    }
    
    // Notifications system
    let notifications = [
      { id: 1, title: 'Deadline approaching', message: 'Stanford MBA application due in 3 days', time: '2 hours ago', unread: true },
      { id: 2, title: 'New AI match', message: 'Harvard MSc CS - 92% match for your profile', time: '5 hours ago', unread: true },
      { id: 3, title: 'Document verified', message: 'Your transcript has been verified successfully', time: '1 day ago', unread: false },
      { id: 4, title: 'Offer received', message: 'Imperial College sent you an offer!', time: '2 days ago', unread: false }
    ];
    
    function toggleNotifications() {
      const dropdown = document.getElementById('notifications-dropdown');
      dropdown.classList.toggle('show');
      
      if (dropdown.classList.contains('show')) {
        renderNotifications();
      }
    }
    
    function renderNotifications() {
      const container = document.getElementById('notifications-list');
      container.innerHTML = notifications.map(notif => `
        <div class="notification-item ${notif.unread ? 'unread' : ''} p-4 border-b border-gray-100" onclick="markAsRead(${notif.id})">
          <div class="flex items-start justify-between">
            <div class="flex-1">
              <p class="font-semibold text-[#0A2540] text-sm mb-1">${notif.title}</p>
              <p class="text-xs text-gray-600 mb-2">${notif.message}</p>
              <p class="text-xs text-gray-400">${notif.time}</p>
            </div>
            ${notif.unread ? '<div class="w-2 h-2 bg-[#5CE1E6] rounded-full ml-2 mt-1"></div>' : ''}
          </div>
        </div>
      `).join('');
    }
    
    function markAsRead(id) {
      notifications = notifications.map(n => n.id === id ? {...n, unread: false} : n);
      renderNotifications();
      updateNotificationBadge();
    }
    
    function markAllAsRead() {
      notifications = notifications.map(n => ({...n, unread: false}));
      renderNotifications();
      updateNotificationBadge();
    }
    
    function updateNotificationBadge() {
      const badge = document.getElementById('notification-badge');
      const unreadCount = notifications.filter(n => n.unread).length;
      badge.style.display = unreadCount > 0 ? 'block' : 'none';
    }
    
    // Toast notification system
    function showToast(message, type = 'success') {
      const container = document.getElementById('toast-container');
      const toast = document.createElement('div');
      toast.className = 'toast-notification';
      
      const colors = {
        success: { bg: '#D1FAE5', text: '#065F46', icon: 'âœ“' },
        error: { bg: '#FEE2E2', text: '#991B1B', icon: 'âœ•' },
        info: { bg: '#DBEAFE', text: '#1E40AF', icon: 'â„¹' },
        warning: { bg: '#FEF3C7', text: '#92400E', icon: 'âš ' }
      };
      
      const color = colors[type];
      toast.style.backgroundColor = color.bg;
      toast.style.color = color.text;
      toast.innerHTML = `
        <div class="flex items-center gap-3">
          <span class="text-2xl">${color.icon}</span>
          <span class="font-semibold">${message}</span>
        </div>
      `;
      
      container.appendChild(toast);
      
      setTimeout(() => {
        toast.style.animation = 'slideOut 0.3s ease';
        setTimeout(() => toast.remove(), 300);
      }, 3000);
    }
    
    // Modal system
    function closeModal(event) {
      if (event.target.id === 'modal-overlay') {
        document.getElementById('modal-overlay').classList.remove('show');
      }
    }
    
    function showDemoModal() {
      const modal = document.getElementById('modal-overlay');
      const modalBody = document.getElementById('modal-body');
      
      modalBody.innerHTML = `
        <h2 class="text-3xl font-bold text-[#0A2540] mb-4">Watch Demo Video</h2>
        <p class="text-gray-600 mb-6">See how NoApplAI streamlines your university application process with intelligent AI-powered features.</p>
        <div class="bg-[#F8F9FC] rounded-xl p-12 text-center">
          <svg class="w-24 h-24 mx-auto text-[#5CE1E6] mb-4" fill="currentColor" viewBox="0 0 20 20">
            <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM9.555 7.168A1 1 0 008 8v4a1 1 0 001.555.832l3-2a1 1 0 000-1.664l-3-2z" clip-rule="evenodd"/>
          </svg>
          <p class="text-gray-600 mb-6">Demo video placeholder</p>
          <button onclick="navigateToPage('dashboard'); closeModal({target: document.getElementById('modal-overlay')})" class="btn-primary px-8 py-3 rounded-xl font-bold">Get Started Now</button>
        </div>
      `;
      
      modal.classList.add('show');
    }
    
    function showProgramDetails(university) {
      showToast(`Viewing ${university} program details`, 'info');
    }
    
    function showApplicationDetails(university) {
      showToast(`Viewing ${university} application details`, 'info');
    }
    
    function uploadNewDocument(type) {
      showToast(`Upload ${type} document`, 'info');
    }
    
    function viewDocument(type) {
      showToast(`Opening ${type} document`, 'info');
    }
    
    function handleFileUpload(event) {
      const files = event.target.files;
      if (files.length > 0) {
        showToast(`Uploading ${files.length} file(s)...`, 'info');
        setTimeout(() => {
          showToast(`${files.length} file(s) uploaded successfully!`, 'success');
        }, 1500);
      }
    }
    
    // Initialize
    document.addEventListener('DOMContentLoaded', function() {
      updateNavLinks('landing');
      updateNotificationBadge();
      
      // Close notifications when clicking outside
      document.addEventListener('click', function(event) {
        const notifButton = document.getElementById('notification-button');
        const notifDropdown = document.getElementById('notifications-dropdown');
        
        if (!notifButton.contains(event.target) && !notifDropdown.contains(event.target)) {
          notifDropdown.classList.remove('show');
        }
      });
    });

    // Element SDK implementation
    const defaultConfig = {
      hero_title: "Your University Applications,\nSimplified by AI",
      hero_subtitle: "NoApplAI streamlines your academic application journey with intelligent recommendations, document management, and deadline tracking â€” all powered by AI.",
      welcome_message: "Welcome back, Qurbanali!",
      dashboard_subtitle: "Here's your application progress and AI-powered recommendations.",
      programs_title: "Discover Programs",
      programs_subtitle: "Search and explore university programs matched to your profile",
      upload_title: "Upload Your Documents",
      upload_subtitle: "Upload and manage all required documents for your applications",
      applications_title: "Your Applications",
      applications_subtitle: "Track and manage all your university applications in one place",
      profile_title: "Your Profile",
      profile_subtitle: "Manage your personal information, account settings, and AI preferences",
      footer_text: "Â© 2024 NoApplAI. All rights reserved."
    };

    async function onConfigChange(config) {
      // Update all editable text content
      const heroTitle = document.getElementById('hero-title');
      if (heroTitle) heroTitle.innerHTML = (config.hero_title || defaultConfig.hero_title).replace(/\n/g, '<br>');
      
      const heroSubtitle = document.getElementById('hero-subtitle');
      if (heroSubtitle) heroSubtitle.textContent = config.hero_subtitle || defaultConfig.hero_subtitle;
      
      const welcomeMessage = document.getElementById('welcome-message');
      if (welcomeMessage) welcomeMessage.textContent = config.welcome_message || defaultConfig.welcome_message;
      
      const dashboardSubtitle = document.getElementById('dashboard-subtitle');
      if (dashboardSubtitle) dashboardSubtitle.textContent = config.dashboard_subtitle || defaultConfig.dashboard_subtitle;
      
      const programsTitle = document.getElementById('programs-title');
      if (programsTitle) programsTitle.textContent = config.programs_title || defaultConfig.programs_title;
      
      const programsSubtitle = document.getElementById('programs-subtitle');
      if (programsSubtitle) programsSubtitle.textContent = config.programs_subtitle || defaultConfig.programs_subtitle;
      
      const uploadTitle = document.getElementById('upload-title');
      if (uploadTitle) uploadTitle.textContent = config.upload_title || defaultConfig.upload_title;
      
      const uploadSubtitle = document.getElementById('upload-subtitle');
      if (uploadSubtitle) uploadSubtitle.textContent = config.upload_subtitle || defaultConfig.upload_subtitle;
      
      const applicationsTitle = document.getElementById('applications-title');
      if (applicationsTitle) applicationsTitle.textContent = config.applications_title || defaultConfig.applications_title;
      
      const applicationsSubtitle = document.getElementById('applications-subtitle');
      if (applicationsSubtitle) applicationsSubtitle.textContent = config.applications_subtitle || defaultConfig.applications_subtitle;
      
      const profileTitle = document.getElementById('profile-title');
      if (profileTitle) profileTitle.textContent = config.profile_title || defaultConfig.profile_title;
      
      const profileSubtitle = document.getElementById('profile-subtitle');
      if (profileSubtitle) profileSubtitle.textContent = config.profile_subtitle || defaultConfig.profile_subtitle;
      
      const footerText = document.getElementById('footer-text');
      if (footerText) footerText.textContent = config.footer_text || defaultConfig.footer_text;
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [],
        borderables: [],
        fontEditable: undefined,
        fontSizeable: undefined
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["hero_title", config.hero_title || defaultConfig.hero_title],
        ["hero_subtitle", config.hero_subtitle || defaultConfig.hero_subtitle],
        ["welcome_message", config.welcome_message || defaultConfig.welcome_message],
        ["dashboard_subtitle", config.dashboard_subtitle || defaultConfig.dashboard_subtitle],
        ["programs_title", config.programs_title || defaultConfig.programs_title],
        ["programs_subtitle", config.programs_subtitle || defaultConfig.programs_subtitle],
        ["upload_title", config.upload_title || defaultConfig.upload_title],
        ["upload_subtitle", config.upload_subtitle || defaultConfig.upload_subtitle],
        ["applications_title", config.applications_title || defaultConfig.applications_title],
        ["applications_subtitle", config.applications_subtitle || defaultConfig.applications_subtitle],
        ["profile_title", config.profile_title || defaultConfig.profile_title],
        ["profile_subtitle", config.profile_subtitle || defaultConfig.profile_subtitle],
        ["footer_text", config.footer_text || defaultConfig.footer_text]
      ]);
    }

    // Initialize Element SDK
    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
